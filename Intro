#include<stdio.h>
int main () {
int a;
int b;
int c;

printf("enter the value of a : ");
scanf("%d", &a);
printf("enter the value of b : ");
scanf("%d",&b);
 
 c=a;
 a=b;
 b=c;
 printf("after swapping : a=%d\n, b=%d\n",a,b );


return 0;
}